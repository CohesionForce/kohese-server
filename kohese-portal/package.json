    "@auth0/angular-jwt": "^5.0.2",
    "@circlon/angular-tree-component": "^11.0.4",
    "@schematics/angular": "^13.3.4",
    "angular-split": "^13.0.1",
    "angular2-virtual-scroll": "^0.4.16",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.1.3",
    "chokidar": "^3.5.2",
    "commonmark": "^0.30.0",
    "core-js": "^3.19.1",
    "d3": "^5.16.0",
    "diff": "^5.0.0",
    "express": "^4.17.1",
    "express-jwt": "^7.6.2",
    "express-rate-limit": "^6.4.0",
